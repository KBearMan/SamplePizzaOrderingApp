<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="gtri.elsys.ahib.pizzatestapp.MainActivity"
  tools:layout_editor_absoluteX="0dp"
  tools:layout_editor_absoluteY="81dp">

  <TextView
    android:id="@+id/textView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="Tip"
    app:layout_constraintEnd_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView2"/>
  <TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:layout_marginStart="8dp"
    android:text="Subtotal:"
    app:layout_constraintBottom_toBottomOf="@+id/edittext_subtotal"
    app:layout_constraintEnd_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"/>
  <TextView
    android:id="@+id/textView4"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="15%"
    android:textSize="24sp"
    app:layout_constraintEnd_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintTop_toTopOf="@+id/textView2"/>
  <TextView
    android:id="@+id/textView3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="20%"
    android:textSize="24sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintTop_toTopOf="@+id/textView4"/>
  <TextView
    android:id="@+id/textView2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="100dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="10%"
    android:textSize="24sp"
    app:layout_constraintEnd_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintHorizontal_bias="0.437"
    app:layout_constraintStart_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintTop_toBottomOf="@+id/edittext_subtotal"/>
  <EditText
    android:id="@+id/edittext_20percenttip"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/edittext_15percenttip"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintTop_toTopOf="@+id/edittext_15percenttip"/>
  <EditText
    android:id="@+id/edittext_subtotal"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.54"
    app:layout_constraintStart_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintTop_toTopOf="parent"/>
  <EditText
    android:id="@+id/edittext_10percenttotal"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginBottom="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/textView11"
    app:layout_constraintEnd_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintTop_toTopOf="@+id/textView11"/>
  <EditText
    android:id="@+id/edittext_15percenttotal"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginBottom="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/edittext_10percenttotal"
    app:layout_constraintEnd_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintTop_toTopOf="@+id/edittext_10percenttotal"/>
  <EditText
    android:id="@+id/edittext_20percenttotal"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/edittext_10percenttotal"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintTop_toTopOf="@+id/edittext_10percenttotal"/>
  <EditText
    android:id="@+id/edittext_15percenttip"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginBottom="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/edittext_10percenttip"
    app:layout_constraintEnd_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintTop_toTopOf="@+id/edittext_10percenttip"/>
  <EditText
    android:id="@+id/edittext_10percenttip"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/textView"
    app:layout_constraintEnd_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintTop_toTopOf="@+id/textView"/>
  <SeekBar
    android:id="@+id/custom_percent_slider"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="0dp"
    android:layout_marginBottom="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintBottom_toBottomOf="@+id/textView12"
    app:layout_constraintEnd_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintTop_toBottomOf="@+id/textView11"/>
  <android.support.constraint.Guideline
    android:id="@+id/vertical25percentguide"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.25"/>
  <android.support.constraint.Guideline
    android:id="@+id/vertical50percentguide"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.5"/>
  <android.support.constraint.Guideline
    android:id="@+id/vertical75percentguide"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.75"/>
  <TextView
    android:id="@+id/textView12"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="Custom"
    app:layout_constraintEnd_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView11"/>
  <TextView
    android:id="@+id/textView11"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="Total"
    app:layout_constraintEnd_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView"/>
  <TextView
    android:id="@+id/textView10"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="CustomTip"
    app:layout_constraintEnd_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView12"/>
  <TextView
    android:id="@+id/textview_custom_percent"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:hint="%"
    app:layout_constraintBottom_toBottomOf="@+id/custom_percent_slider"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintTop_toBottomOf="@+id/edittext_20percenttotal"/>
  <TextView
    android:id="@+id/textView8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:text="CustomTotal"
    app:layout_constraintBottom_toBottomOf="@+id/textView10"
    app:layout_constraintEnd_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintStart_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintTop_toTopOf="@+id/textView10"/>
  <EditText
    android:id="@+id/edittext_custompercenttip"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/textView10"
    app:layout_constraintEnd_toStartOf="@+id/vertical50percentguide"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="@+id/vertical25percentguide"
    app:layout_constraintTop_toBottomOf="@+id/textView12"/>
  <EditText
    android:id="@+id/edittext_custompercenttotal"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="0dp"
    android:ems="10"
    android:inputType="numberDecimal"
    app:layout_constraintBottom_toBottomOf="@+id/textView8"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="@+id/vertical75percentguide"
    app:layout_constraintTop_toTopOf="@+id/textView8"/>
</android.support.constraint.ConstraintLayout>
